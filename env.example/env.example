NODE_ENV=production                        # 运行环境（production | development）
REDIS_URL=redis://redis:6379               # Redis 连接地址
ENABLE_REDIS=true                          # 是否启用 Redis（true/false，生产建议 true）
TRUST_PROXY=1                              # 是否信任反向代理头；反代(frps/frpc+Nginx/Caddy/Traefik)时设为 1，直连运行可设为 false
JWT_SECRET=CHANGE_ME_TO_A_SECURE_32_PLUS_CHAR_STRING    # JWT 签名密钥，务必更换为强随机字符串
ADMIN_SECRET=nameadmin                     # 管理员口令，务必更换为强口令
# DETECTED_CPU_COUNT=4                     # （可选）声明服务器 CPU 核心数（未填写则自动检测）
# DETECTED_MEMORY_GB=8                     # （可选）声明服务器内存（GB，未填写则自动检测）
TZ=Asia/Shanghai                           # 时区设置（如在国内服务器建议设为 Asia/Shanghai）
